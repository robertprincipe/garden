  "scripts": {
    "build": "next build",
    "bv": "tsc --noEmit",
    "canary": "pnpm i react@canary react-dom@canary next@canary",
    "canary:ts": "pnpm canary && pnpm i -D typescript@beta",
    "check": "run-s type-check lint format:check test",
    "clean": "git clean -xdf .next",
    "db": "drizzle-kit --help",
    "db:check": "drizzle-kit check:pg --config=drizzle.config.ts",
    "db:drop": "drizzle-kit drop --config=drizzle.config.ts",
    "db:generate": "drizzle-kit generate:pg --config=drizzle.config.ts",
    "db:introspect": "drizzle-kit introspect:pg --config=drizzle.config.ts",
    "db:migrate": "node ./src/data/db/migrate.mjs",
    "db:push": "drizzle-kit push:pg --config=drizzle.config.ts",
    "db:seed": "tsx src/data/env/seed.ts",
    "db:studio": "drizzle-kit studio --port 5000 --verbose --config=drizzle.config.ts",
    "dev": "next dev --port=3001",
    "dev:email": "email dev",
    "edge": "pnpm latest && pnpm canary",
    "email": "email dev --dir src/data/mail -p 3001",
    "format": "pnpm format:write",
    "format:check": "prettier --check \"**/*.{js,ts,tsx,md,mdx,json}\" --cache",
    "format:write": "prettier --write \"**/*.{js,ts,tsx,md,mdx,json}\" --cache",
    "postinstall": "pnpm db:generate && pnpm db:push",
    "latest": "pnpm up --latest && pnpm i next-intl@3.0.0-beta.19",
    "lint": "pnpm lint:next && pnpm lint:i18n",
    "lint:fix": "next lint --fix",
    "lint:i18n": "eslint --fix --ext .json --format node_modules/eslint-plugin-i18n-json/formatter.js src/i18n/messages",
    "lint:next": "next lint",
    "shadcn": "pnpm dlx shadcn-ui@latest add",
    "start": "next start",
    "stripe:listen": "stripe listen --forward-to localhost:3000/api/webhooks/stripe --latest",
    "test": "jest --passWithNoTests",
    "type-check": "next build",
    "typecheck": "next build"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./jest.setup.ts"
    ]
  },
